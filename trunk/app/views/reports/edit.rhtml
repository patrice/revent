<h1>Editing report</h1>

<% form_for(:report, @report, :url => { :action => 'update', :id => @report }, :html => { :multipart => true } ) do |f| %>
  <%= render :partial => 'form', :locals => {:f => f} %>
  <%= submit_tag 'Edit' %>
<% end %>

<%= link_to 'Show', :action => 'show', :id => @report %> |
<%= link_to 'Back', :action => 'list' %>
